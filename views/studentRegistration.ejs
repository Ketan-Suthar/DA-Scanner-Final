<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colorlib Templates">
    <meta name="author" content="Colorlib">
    <meta name="keywords" content="Colorlib Templates">

    <!-- Title Page-->
    <title>Student Registration</title>
    
    <!-- Icons font CSS-->
    <link href="css/Registration_CSS/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <link href="css/Registration_CSS/font-awesome.min.css" rel="stylesheet" media="all">
    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">

    <!-- Vendor CSS-->
    <link href="css/Registration_CSS/select2.min.css" rel="stylesheet" media="all">
    <link href="css/Registration_CSS/daterangepicker.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="/css/Login_CSS/Login_css.css" rel="stylesheet">
    <link href="css/Registration_CSS/main.css" rel="stylesheet" media="all">
</head>

<body>
    <img src="/pic/Wave2.jpg" class="wave"> 
    <%
        var fname,lname,email,stuID;
        if(data){
            fname=data.fname;
            lname=data.lname;
            email=data.emailId;
            stuID=data.stuID;
        }
        
        var hey = "asdasd";
    
    %>
    <!--<h1 style="color:black"> <%=fname%> </h1>-->
    <!--<div class="page-wrapper bg-blue p-t-100 p-b-100 font-robo">-->
        <div class="size">
            <div class="card container">
                <div>
                    <img src="https://www.daiict.ac.in/wp-content/uploads/2019/01/logo-2018.png" class="img1 img-responsive"/>
                </div>
               
                    <center><h2 class="title">Student Registration</h2></center>
                    <center><h4 class="title" style="color:red"><%if(error){%> <%=error%> <%}%></h4></center>
                    <form method="POST" action="/users/registerStudent" onSubmit = "return validate(this)">
                        <div class="input-group">
                            <input class="input--style-1" type="text" placeholder="FIRST NAME"  onkeypress="return isCharKey(event)" name="fname" id="fname"  value="<%=fname%>" required>
                        </div>
                        <br/>
                        <div class="input-group">
                                <input class="input--style-1" type="text" placeholder="LAST NAME" name="lname" id="lname" value="<%=lname%>" required>
                        </div>
                        <br/>
                        
                        <br/>
                        <div class="input-group">
                                 <input class="input--style-1" type="text" placeholder="Student ID Number" onkeypress="return isNumberKey(event)"  onfocusout="IDCheck()" maxlength="9"  name="stuID" id="stuID"  value="<%=stuID%>" required>
                        </div>
                        <span id="error-ID" style="color:red;"></span>
                        <br/>
                        <div  class="input-group">
                            <input class="input--style-1" type="text" placeholder="Email" name="emailId" id="emailId" onfocusout="EmailCheck()"  value="<%=email%>" readonly="readonly" >
                        </div>

                        <span id="error-Email" style="color:red;"></span>   
                        <br/>
                        <div class="input-group">
                                <input class="input--style-1" type="password" placeholder="Password"  name="password" id="Pass1" required>
                       </div>
                       <br/>
                       <div class="input-group">
                            <input class="input--style-1" type="password" placeholder="Re-Type Password" onfocusout="validate()" name="confirm_password" id="Pass2" required>
                        </div>
                        <span id="error-Password" style="color:red;"></span>   
                        <br/>
                        <br/>
                        <div class="container" style="background-color:#f1f1f1">
                          <button type="submit"  name="submitButton" id="submitButton">SUBMIT</button>
                        </div>
                    </form>
            </div>
        </div>
    <script src="/js/Registration_JS/reg.js"></script>
    <!-- Jquery JS-->
    
    <script src="/js/Registration_JS/jquery.min.js"></script>
    <!-- Vendor JS-->
    <script src="/js/Registration_JS/select2.min.js"></script>
    <script src="/js/Registration_JS/moment.min.js"></script>
    <script src="/js/Registration_JS/daterangepicker.js"></script>
   
    <script src="/js/Registration_JS/global.js"></script>

</body><!-- This templates was made by Colorlib (https://colorlib.com) -->

</html>
<!-- end document-->
